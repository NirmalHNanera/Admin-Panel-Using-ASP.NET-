@using System.Data;
@model DataTable
<table class="table">
    <thead>
        <tr>
            <th scope="col">Branch ID</th>
            <th scope="col">Branch Name</th>
            <th scope="col">Branch Code</th>
            <th scope="col">Created</th>
            <th scope="col">Modified</th>

        </tr>
    </thead>
    <tbody>
        @{
            foreach (DataRow dr in Model.Rows)
            {
                <tr>
                    <td>@dr["BranchID"]</td>
                    <td>@dr["BranchName"]</td>
                    <td>@dr["BranchCode"]</td>
                    <td>@dr["Created"]</td>
                    <td>@dr["Modified"]</td>
                </tr>
            }
        }

</table>


